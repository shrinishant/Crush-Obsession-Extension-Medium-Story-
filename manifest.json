{
    "name": "Crush Obsession",
    "description": "Build a basic Extension!",
    "version": "2",
    "manifest_version": 3,
    "background": {
        "service_worker": "background.js"
      },
    "permissions": [
        "activeTab", "scripting", "storage"
    ],
    "action": {
      "default_popup": "popup.html",
      "default_title": "Crush Obsession"
    }
  }